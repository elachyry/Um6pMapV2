    "@fastify/multipart": "^8.0.0",
    "@fastify/rate-limit": "^9.0.1",
    "@fastify/static": "^6.12.0",
    "@fastify/websocket": "^8.3.1",
    "@prisma/client": "^5.7.1",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.8.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.2",
    "multer": "^2.0.2",
